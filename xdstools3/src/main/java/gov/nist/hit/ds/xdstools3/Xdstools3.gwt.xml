<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="xdstools3">

    <source path="client" />
    <source path="shared" />

    <!-- ======================== Inherit GWT ======================= -->
    <inherits name='com.google.gwt.user.User' />
    <inherits name='com.google.gwt.rpc.RPC' />
    <inherits name="com.google.gwt.resources.Resources" />
    <!--   <inherits name="org.gwtbootstrap3.GwtBootstrap3"/> -->


    <!-- ===================== Inherit SmartGWT ===================== -->
    <inherits name="com.smartgwt.SmartGwt"/>
    <inherits name="com.smartgwt.tools.SmartGwtTools"/>
    <inherits name="com.smartgwt.SmartGwtNoTheme"/>
    <inherits name="com.smartclient.theme.enterprise.Enterprise"/>
    <!--  <inherits name="com.smartclient.theme.silverwave.SilverWave" /> -->
    <!--<inherits name="com.smartclient.theme.enterpriseblue.EnterpriseBlue"/>-->


    <!-- ====== Inherit GWT Injection, Activity, Places modules ====== -->
    <inherits name="com.google.gwt.inject.Inject"/>
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="com.google.gwt.place.Place" />


    <!-- ================= Inherit v3 modules ================= -->
    <!-- begin repository related -->
    <inherits name='gov.nist.hit.ds.repository.ui.RepositoryWidgetsUI' />
    <!-- end repository related -->

    <!-- ================= Inherit v2 module ================= -->
    <inherits name='gov.nist.toolkit.xdstools2.Xdstools2_NOEP' />


    <!-- ============ Specify the app entry point class ============== -->
    <entry-point class="gov.nist.hit.ds.xdstools3.client.Xdstools3EP" />


    <!-- Uncomment this line to force GWT compiler to only generate javascript
        for Ie10 browsers. Chrome is always supported. -->
   <!-- <set-property name="user.agent" value="ie9" /> -->


    <!-- Uncomment this line to force GWT compiler to only generate javascript
        for Firefox browsers. Chrome is always supported. -->
    <!-- <set-property name="user.agent" value="gecko1_8" /> -->


    <!-- ======================= GWT logging ======================= -->
     <inherits name="com.google.gwt.logging.Logging"/>
    <inherits name="gov.nist.toolkit.results.results"/>
    <set-property name="gwt.logging.logLevel" value="INFO"/>
     <set-property name="gwt.logging.enabled" value="TRUE"/>
     <set-property name="gwt.logging.popupHandler" value="DISABLED" />

 </module>


