  <target name="compile.module.soap" depends="compile.module.soap.production,compile.module.soap.tests" description="Compile module soap"/>
  
  <target name="compile.module.soap.production" depends="register.custom.compilers,compile.module.docref,compile.module.dsig,compile.module.security-common,compile.module.xds-exceptions,compile.module.utilities,compile.module.common-datatypes,compile.module.error-recording,compile.module.installation,compile.module.tk,compile.module.registry-support,compile.module.site-management,compile.module.actortransaction,compile.module.validators-support,compile.module.registry-metadata,compile.module.wssetool" description="Compile module soap; production classes">
    <mkdir dir="${soap.output.dir}"/>
    <javac2 destdir="${soap.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.soap}/javac">
      <compilerarg line="${compiler.args.soap}"/>
      <bootclasspath refid="soap.module.bootclasspath"/>
      <classpath refid="soap.module.production.classpath"/>
      <src refid="soap.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.soap"/>
    </javac2>
    
    <copy todir="${soap.output.dir}">
      <fileset dir="${module.soap.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.soap.tests" depends="register.custom.compilers,compile.module.soap.production" description="compile module soap; test classes" unless="skip.tests">
    <mkdir dir="${soap.testoutput.dir}"/>
    <javac2 destdir="${soap.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.soap}/javac">
      <compilerarg line="${compiler.args.soap}"/>
      <bootclasspath refid="soap.module.bootclasspath"/>
      <classpath refid="soap.module.classpath"/>
      <src refid="soap.module.test.sourcepath"/>
      <patternset refid="excluded.from.compilation.soap"/>
    </javac2>
    
    <copy todir="${soap.testoutput.dir}">
