  <!-- Run GWT compiler for GWT module ${gwt.module.name} -->
  <target name="run.gwt.compiler.soap" description="Run GWT compiler">
    <java fork="true" jvm="${module.jdk.bin.soap}/java" classname="com.google.gwt.dev.Compiler">
