        <path refid="installation.module.sourcepath"/>
        <path refid="tk.module.sourcepath"/>
        <path refid="registry-support.module.sourcepath"/>
        <path refid="site-management.module.sourcepath"/>
        <path refid="actortransaction.module.sourcepath"/>
        <path refid="validators-support.module.sourcepath"/>
        <path refid="registry-metadata.module.sourcepath"/>
        <path refid="wssetool.module.sourcepath"/>
        <path refid="soap.module.classpath"/>
      </classpath>
      <arg value="-logLevel"/>
      <arg value="WARN"/>
      <arg value="-war"/>
      <arg value="${gwt.compiler.output.soap}"/>
      <arg value="-style"/>
      <arg value="DETAILED"/>
      <arg value="${gwt.module.name}"/>
    </java>
  </target>
  
