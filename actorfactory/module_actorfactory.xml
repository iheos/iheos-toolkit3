  <!-- Run GWT compiler for GWT module ${gwt.module.name} -->
  <target name="run.gwt.compiler.actorfactory" description="Run GWT compiler">
    <java fork="true" jvm="${module.jdk.bin.actorfactory}/java" classname="com.google.gwt.dev.Compiler">
      <jvmarg line="-Xmx256m"/>
      <classpath>
        <pathelement location="${gwt.sdk.home.actorfactory}/gwt-dev.jar"/>
        <path refid="actorfactory.module.sourcepath"/>
        <path refid="actortransaction.module.sourcepath"/>
        <path refid="env-settings.module.sourcepath"/>
        <path refid="installation.module.sourcepath"/>
        <path refid="tk.module.sourcepath"/>
        <path refid="utilities.module.sourcepath"/>
        <path refid="common-datatypes.module.sourcepath"/>
        <path refid="xds-exceptions.module.sourcepath"/>
        <path refid="http.module.sourcepath"/>
        <path refid="error-recording.module.sourcepath"/>
        <path refid="registry-metadata.module.sourcepath"/>
        <path refid="docref.module.sourcepath"/>
        <path refid="registry-support.module.sourcepath"/>
        <path refid="results.module.sourcepath"/>
        <path refid="sim-common.module.sourcepath"/>
        <path refid="site-management.module.sourcepath"/>
        <path refid="validators-support.module.sourcepath"/>
        <path refid="actorfactory.module.classpath"/>
      </classpath>
      <arg value="-logLevel"/>
      <arg value="WARN"/>
      <arg value="-war"/>
      <arg value="${gwt.compiler.output.actorfactory}"/>
      <arg value="-style"/>
      <arg value="DETAILED"/>
      <arg value="${gwt.module.name}"/>
    </java>
  </target>
  
  <target name="compile.gwt.actorfactory" depends="compile.module.actorfactory" description="Compile all GWT modules in module actorfactory">
    <antcall target="run.gwt.compiler.actorfactory">
      <param name="gwt.module.name" value="gov.nist.toolkit.actorfactory.actorfactory"/>
    </antcall>
  </target>
