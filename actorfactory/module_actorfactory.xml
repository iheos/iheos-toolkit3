  <target name="compile.module.actorfactory" depends="compile.module.actorfactory.production,compile.module.actorfactory.tests" description="Compile module actorfactory"/>
  
  <target name="compile.module.actorfactory.production" depends="register.custom.compilers,compile.module.actortransaction,compile.module.env-settings,compile.module.installation,compile.module.tk,compile.module.utilities,compile.module.common-datatypes,compile.module.xds-exceptions,compile.module.http,compile.module.error-recording,compile.module.registry-metadata,compile.module.docref,compile.module.registry-support,compile.module.results,compile.module.sim-common,compile.module.site-management,compile.module.validators-support" description="Compile module actorfactory; production classes">
    <mkdir dir="${actorfactory.output.dir}"/>
    <javac2 destdir="${actorfactory.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.actorfactory}/javac">
      <compilerarg line="${compiler.args.actorfactory}"/>
      <bootclasspath refid="actorfactory.module.bootclasspath"/>
      <classpath refid="actorfactory.module.production.classpath"/>
      <src refid="actorfactory.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.actorfactory"/>
    </javac2>
    
    <copy todir="${actorfactory.output.dir}">
      <fileset dir="${module.actorfactory.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
